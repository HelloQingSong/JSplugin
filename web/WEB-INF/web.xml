<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>changeStyleFunction</servlet-name>
        <servlet-class>com.js.produce.core.ChangeStyleFunctionServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>changeStyleFunction</servlet-name>
        <url-pattern>/changeStyleJS</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>toUpperCaseFunction</servlet-name>
        <servlet-class>com.js.produce.core.toUpperCaseFunctionServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>toUpperCaseFunction</servlet-name>
        <url-pattern>/toUpperCaseJS</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>radioRevokedFunction</servlet-name>
        <servlet-class>com.js.produce.core.RadioRevokedFunctionServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>radioRevokedFunction</servlet-name>
        <url-pattern>/radioRevokedJS</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>com.js.produce.test.TestServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
</web-app>